CREATE OR REPLACE PROCEDURE insertar_transaccion (
    p_cod_sucursal      IN OUT transacpagos.cod_sucursal%TYPE,
    p_id_cliente        IN OUT transacpagos.clienteid%TYPE,
    p_tipo_prestamo     IN OUT transacpagos.tipo_prestamoid%TYPE,
    p_fecha_transaccion IN OUT transacpagos.fecha_transaccion%TYPE,
    p_monto_del_pago    IN OUT transacpagos.monto_del_pago%TYPE,
    p_fecha_insercion   IN OUT transacpagos.fecha_insercion%TYPE,
    p_status            IN OUT transacpagos.status%TYPE) IS  
BEGIN
INSERT INTO transacpagos (id_transaccion,cod_sucursal, clienteid, tipo_prestamoid, fecha_transaccion, monto_del_pago, fecha_insercion, usuario,status)
VALUES (secuencia_transaccion.nextval, p_cod_sucursal, p_id_cliente, p_tipo_prestamo, p_fecha_transaccion, p_monto_del_pago, p_fecha_insercion, user,p_status);
END insertar_transaccion;

DECLARE
    v_cod_sucursal       transacpagos.cod_sucursal%TYPE:= 1;
    v_id_cliente         transacpagos.clienteid%TYPE:=1;
    v_tipo_prestamo      transacpagos.tipo_prestamoid%TYPE:=1;
    v_fecha_transaccion  transacpagos.fecha_transaccion%TYPE:=TO_DATE('02/10/2021','DD/MM/YYYY');
    v_monto_del_pago     transacpagos.monto_del_pago%TYPE:= 1000;
    v_fecha_insercion    transacpagos.fecha_insercion%TYPE:=TO_DATE('02/10/2021','DD/MM/YYYY');
    v_status             transacpagos.status%TYPE:='N';
BEGIN
insertar_transaccion (v_cod_sucursal,v_id_cliente,v_tipo_prestamo,v_fecha_transaccion,v_monto_del_pago,v_fecha_insercion,v_status); 
END;


BEGIN
insertar_transaccion (3, 3, 3, 2, TO_DATE('19/11/2020','DD/MM/YYYY'), 100.00, TO_DATE('17/11/2020','DD/MM/YYYY'), 'N');
END;