CREATE SEQUENCE secuencia_cliente
    MINVALUE 1
    START WITH 1
    INCREMENT BY 1
    NOCACHE;
CREATE SEQUENCE secuencia_prestamo
    MINVALUE 1
    START WITH 1
    INCREMENT BY 1
    NOCACHE;
CREATE SEQUENCE secuencia_sucursal
    MINVALUE 1
    START WITH 1
    INCREMENT BY 1
    NOCACHE;
CREATE SEQUENCE secuencia_profesion
    MINVALUE 1
    START WITH 1
    INCREMENT BY 1
    NOCACHE;
CREATE SEQUENCE secuencia_correo
    MINVALUE 1
    START WITH 1
    INCREMENT BY 1
    NOCACHE;
CREATE SEQUENCE secuencia_telefono
    MINVALUE 1
    START WITH 1
    INCREMENT BY 1
    NOCACHE;
CREATE SEQUENCE secuencia_transaccion
    MINVALUE 1
    START WITH 1
    INCREMENT BY 1
    NOCACHE;

drop sequence secuencia_cliente;
drop sequence secuencia_prestamo;
drop sequence secuencia_sucursal;
drop sequence secuencia_profesion;
drop sequence secuencia_correo;
drop sequence secuencia_telefono;
